{
  "name": "hartsoft/genlite",
  "type": "project",
  "license": "MIT",
  "description": "My WordPress boilerplate with modern development tools, easier configuration, and an improved folder structure",
  "keywords": [
    "composer",
    "wordpress",
    "wp",
    "wp-config"
  ],
  "repositories": [
     {
      "type": "composer",
      "url": "https://wpackagist.org"
    }
  ],
  "require": {
    "wpackagist-plugin/duplicate-post": "^4.1",
    "wpackagist-plugin/post-types-order": "^1.9",
    "wpackagist-plugin/redirection": "^5.1",
    "wpackagist-plugin/svg-support": "^2.3",
    "wpackagist-plugin/timber-library": "^1.18",
    "wpackagist-plugin/tiny-compress-images": "^3.2",
    "wpackagist-plugin/wordpress-seo": "^16.3",
    "wpackagist-plugin/wp-mail-smtp": "^2.8",
    "wpackagist-plugin/user-role-editor": "^4.59",
    "wpackagist-plugin/wordfence": "^7.5",
    "palmiak/timber-acf-wp-blocks": "^1.14",
    "wpackagist-plugin/woocommerce": "^5.4",
    "wpackagist-plugin/regenerate-thumbnails": "^3.1",
    "wpackagist-plugin/instagram-feed":"2.9.3.1"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.6.0",
    "roave/security-advisories": "dev-master"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "installer-paths": {
      "./mu-plugins/{$name}/": ["type:wordpress-plugin"]
    }
  },
  "scripts": {
    "post-root-package-install": [
      "php -r \"copy('.env.example', '.env');\""
    ],
    "test": [
      "phpcs"
    ]
  }
}
