{% extends 'base.twig' %}

{% block content %}

    <div class="container">
        <div class="row">

            <div class="col-12">

                {% for item in products %}

                    <img style="height: 100px; width: 100px;" src="{{ item.thumbnail.src }}"/>
                    <a href="{{ item.link }}">{{ item.title }}</a>

                {% endfor %}

                {# {{ dump(products) }} #}
               
            </div>

        </div>
    </div>

{% endblock %}
