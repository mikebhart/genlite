<!doctype html>
<html lang="en">

<head>
    <meta charset="{{ site.charset }}" />
     {% if post.genlite_seo_description is not empty %}
        <meta name="description" content="{{ post.genlite_seo_description }}">        
    {% endif %} 

    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <link rel="icon" type="image/png" sizes="48x48" href="{{ header_favicon.url }}">
    <link rel="icon" type="image/png" sizes="192x192" href="{{ header_favicon.url }}">
    <link rel="apple-touch-icon" type="image/png" sizes="167x167" href="{{ header_favicon.url }}">
    <link rel="apple-touch-icon" type="image/png" sizes="180x180" href="{{ header_favicon.url }}">
