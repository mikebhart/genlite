<!doctype html>
<html lang="en">

<head>

    {% if script_code.code_head is not empty %}

        {{ script_code.code_head }}

    {% endif %}

    <meta charset="{{ site.charset }}" />

     {% if post.genlite_seo_description is not empty %}
        <meta name="description" content="{{ post.genlite_seo_description }}">        
    {% endif %} 

    <link rel='preload' href='{{ theme.link }}/dist/assets/fonts/karla-v30-latin-regular.woff2' as='font' type='font/woff2' crossorigin>
    <link rel='preload' href='{{ theme.link }}/dist/assets/fonts/rubik-v26-latin-regular.woff2' as='font' type='font/woff2' crossorigin>

    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <link rel="pingback" href="{{ site.pingback_url }}" />
    <link rel="profile" href="http://gmpg.org/xfn/11">

    {{ function('wp_head') }}
